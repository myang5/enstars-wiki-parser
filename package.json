{
  "name": "enstars-wiki-parser",
  "version": "1.0.0",
  "description": "A simple formatter that takes in translators' dialogue and converts it into source code for a new page on the Ensemble Stars wikia. Understanding and uploading the wiki code shouldn't be a roadblock for translators to share their hard work!",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "transpile": "npx babel --watch src --out-dir dist --presets react-app/prod",
    "dev": "webpack-dev-server",
    "build": "webpack --mode production",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myang5/enstars-wiki-parser.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/myang5/enstars-wiki-parser/issues"
  },
  "homepage": "https://github.com/myang5/enstars-wiki-parser#readme",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@ckeditor/ckeditor5-autoformat": "^17.0.0",
    "@ckeditor/ckeditor5-autosave": "^20.0.0",
    "@ckeditor/ckeditor5-basic-styles": "^17.0.0",
    "@ckeditor/ckeditor5-dev-utils": "^12.0.9",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^8.0.9",
    "@ckeditor/ckeditor5-editor-balloon": "^17.0.0",
    "@ckeditor/ckeditor5-essentials": "^17.0.0",
    "@ckeditor/ckeditor5-link": "^17.0.0",
    "@ckeditor/ckeditor5-list": "^20.0.0",
    "@ckeditor/ckeditor5-paragraph": "^17.0.0",
    "@ckeditor/ckeditor5-paste-from-office": "^17.0.0",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@ckeditor/ckeditor5-theme-lark": "^17.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^3.1.2",
    "clean-webpack-plugin": "^3.0.0",
